<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>com.facebook.presto</groupId>
    <artifactId>presto-assembly</artifactId>
    <version>0.139.2-cdh5.5.2</version>
  </parent>

  <groupId>com.facebook.presto.csd</groupId>
  <artifactId>PRESTO</artifactId>
  <version>0.139.2-cdh5.5.2</version>
  <name>presto-csd</name>

  <properties>
    <presto.version>${project.version}</presto.version>
  </properties>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.6</version>
      </plugin>
    </plugins>

    <resources>
      <resource>
        <directory>src/main/resources</directory>
        <filtering>true</filtering>
      </resource>
    </resources>
   </build>
</project>
